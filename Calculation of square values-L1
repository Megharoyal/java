import java.util.Scanner;
public class square
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int n;
        do{
        n=sc.nextInt();
        if(n!=-1)
        {
            int d=n*n;
            System.out.println(d);
        }
        else
        {
            System.out.println("Invalid input");
        }
        }while(n!=-1);
    
        
    }
}
